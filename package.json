{
  "name": "gregpedro-backend",
  "version": "1.0.0",
  "description": "Backend for Dr. Greg Pedro's dental practice website",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "supabase start",
    "build": "npm install -g supabase && supabase init --force",
    "postbuild": "supabase db push && supabase db exec -f supabase/seed/seed_data.sql",
    "db:push": "supabase db push",
    "db:seed": "supabase db exec -f supabase/seed/seed_data.sql",
    "functions:serve": "supabase functions serve"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7"
  },
  "devDependencies": {
    "supabase": "^2.22.4"
  },
  "private": true
}
